
@{
    ViewBag.Title = "LiveGet";
}

@*<button class="btn btn-warning" id="clicklive">LiveGet</button>*@

<div style="border-bottom: 1px solid white;">
    <button type="button" class="btn btn-danger" id="liveErrorColor" style="height:30px; margin-left:25px; line-height:0;">Error <span class="badge" id="liveErrorCount"></span></button>
    <button type="button" class="btn btn-warning" id="liveCrashColor" style="height:30px; margin-left:5px;line-height:0;">Crash <span class="badge" id="liveCrashCount"></span></button>
</div>
<div id="liveGetSend" style=" background-color: #fff;"></div>

<div id="load_walk" name="Load">
    <img src="/image/loading.gif" id="load" alt="" title="Wplease Wait...">
</div>
<link href="~/Content/Style/Load.css" rel="stylesheet" />

<script type="text/javascript">
    $(document).ready(function () {
        $('#load_walk').css("display", "block");
        $.post('/LiveGet/GetLiveSensor', {}, function (Response) {
            $('#liveGetSend').html("");
            $('#liveGetSend').html(Response);
            $('#load_walk').css("display", "none");
        });
    });
</script>

<style>
    body {
        background-color: #333;
    }

    .container {
        width: 80%;
        margin: 15px auto;
    }
</style>